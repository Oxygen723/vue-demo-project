    "axios": "^1.9.0",
    "dayjs": "^1.11.13",
    "element-plus": "^2.9.10",
    "normalize.css": "^8.0.1",
    "pinia": "^3.0.2",
    "vue": "^3.5.13",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@types/node": "^20.17.47",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/tsconfig": "^0.7.0",
    "fast-glob": "^3.3.3",
    "sass-embedded": "^1.88.0",
    "typescript": "~5.8.3",
    "unocss": "^66.1.2",
    "unplugin-auto-import": "^19.2.0",
    "unplugin-vue-components": "^28.5.0",
    "vite": "^6.3.5",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-vue-devtools": "^7.7.6",
    "vue-tsc": "^2.2.8"
