{
  "name": "vue3-ts-template",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build && node ./src/build/build.js",
    "build:not-ts": "vite build && node ./src/build/build.js",
    "build:test": "npm run build:not-ts --mode test",
    "build:prod": "npm run build --mode prod",
    "preview": "vite preview"
  },
  "dependencies": {
